{
  "name": "appsbrekekecom",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "lint": "eslint --ignore-path=.gitignore .",
    "pret": "js-import-sort --silent=1 --path src/**/*.js && prettier --ignore-path=.gitignore --loglevel=silent --write \"**/*.{html,scss,js,json,md}\"",
    "deploy": "yarn build && ssh apps.brekeke.com \"rm -rf /var/www/apps/*\" && scp -r build/. apps.brekeke.com:/var/www/apps"
  },
  "dependencies": {
    "history": "^4.9.0",
    "humanize-duration": "^3.18.0",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "mobx-react-router": "^4.0.7",
    "normalize.css": "^8.0.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@craco/craco": "^5.1.0",
    "circular-dependency-plugin": "^5.0.2",
    "js-import-sort": "^1.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "react-scripts": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
